#include<stdio.h>
int main()
{
    int i,num;
    int cont=0;
    for(i=0;i<10;i++){
        printf("Digite o numero %d: ",i+1);
        scanf("%d",&num);
        if(num%5==0 && num%3==0)
        {
            cont++;
        }
    }
    printf("Quantidade de numeros divisiveis por 5 e 3: %d\n",cont);
    return 0;
}
